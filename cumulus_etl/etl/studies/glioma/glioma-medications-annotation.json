{
  "$defs": {
    "GliomaChemotherapyClass": {
      "enum": [
        "NOT_MENTIONED",
        "ALKYLATING_AGENT",
        "PLATINUM_AGENT",
        "VINCA_ALKALOID",
        "TOP1_INHIBITOR",
        "ANTIMETABOLITE",
        "MULTI_AGENT_CHEMOTHERAPY",
        "OTHER"
      ],
