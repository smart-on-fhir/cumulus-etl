    "GliomaTargetedTherapyMention": {
      "properties": {
        "has_mention": {
          "default": false,
          "description": "Whether there is any mention of this variable in the text.",
          "title": "Has Mention",
          "type": "boolean"
        },
        "spans": {
          "description": "The text spans where this variable is mentioned.",
          "items": {
            "type": "string"
