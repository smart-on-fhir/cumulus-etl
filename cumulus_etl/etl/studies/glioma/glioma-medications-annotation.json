      "type": "string"
    },
    "RxTreatmentPhase": {
      "description": "Treatment Phase",
      "enum": [
        "NOT_MENTIONED",
        "Induction therapy",
        "Maintenance therapy",
        "Rescue therapy",
        "None of the above"
